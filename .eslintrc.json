{
  "parserOptions": {
    "ecmaFeatures": { "experimentalObjectRestSpread": true }
  },
  "plugins": [
    "import"
  ],
  "settings": {
    "import/resolver": "webpack"
  },
  "extends": "airbnb",
  "rules": {
    "react/prop-types": 0,
    "max-len": ["error", 150],
    "no-shadow": 0,
    "import/named": 2
  }
}
