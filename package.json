{
  "name": "opendota-ui",
  "description": "Dota 2 Data Web UI",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "webpack": "NODE_ENV=production webpack",
    "start": "webpack-dev-server --hot",
    "build": "npm install && npm run lintnofix && npm run webpack",
    "lint": "eslint . --ext .js,.jsx --fix",
    "lintnofix": "eslint . --ext .js,.jsx",
    "test": "NODE_PATH=./src NODE_ENV=test mocha --ui bdd --reporter nyan -r test/initializeJsdom.js $(find src -name '*.spec.*')",
    "test:watch": "npm test -- --watch --watch-extensions jsx,js",
    "update:emoticons": "npm r dota2-emoticons && npm i dota2-emoticons && node dev/updateEmoticons.js"
  },
  "greenkeeper": {
    "label": "type: greenkeeper"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/odota/ui"
  },
  "dependencies": {
    "abcolor": "^0.5.5",
    "c3": "^0.4.15",
    "dota2-emoticons": "^1.0.1",
    "dotaconstants": "^5.11.0",
    "fuzzy": "^0.1.3",
    "heatmap.js": "^2.0.5",
    "isomorphic-fetch": "^2.2.1",
    "json2csv": "^3.10.0",
    "lodash": "^4.17.4",
    "long": "^3.2.0",
    "material-ui": "^0.19.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.1.3",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-style-proptype": "^3.0.0",
    "react-tooltip": "^3.3.0",
    "redux": "^3.7.2",
    "redux-responsive": "^4.3.2",
    "redux-thunk": "^2.2.0",
    "seamless-immutable": "^7.1.2",
    "styled-components": "^2.2.0",
    "uuid": "^3.1.0",
    "wordcloud": "^1.0.6"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.2.0",
    "file-loader": "^1.0.0",
    "fs-extra": "^4.0.1",
    "simple-vdf": "^1.1.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "moduleRoots": [
    "./src",
    "./node_modules"
  ]
}
